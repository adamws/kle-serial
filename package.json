{
  "name": "kle-serial",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve --port 3000",
    "build": "vue-cli-service build --dest build",
    "lint": "vue-cli-service lint",
    "postinstall": "patch-package && cd node_modules/@ijprest/kle-serial && npm run build"
  },
  "dependencies": {
    "@ijprest/kle-serial": "git+https://github.com/ijprest/kle-serial.git",
    "core-js": "~3.6.5",
    "element-plus": "^1.0.1-beta.27",
    "file-saver": "^2.0.5",
    "prismjs": "^1.25.0",
    "vue": "~3.0.0-0",
    "vue-prism-editor": "^2.0.0-alpha.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/cli-plugin-eslint": "^4.5.11",
    "@vue/cli-service": "^4.5.11",
    "@vue/compiler-sfc": "~3.0.0-0",
    "babel-eslint": "~10.1.0",
    "eslint": "~6.7.2",
    "eslint-plugin-vue": "~7.0.0-0",
    "patch-package": "^6.2.2",
    "typescript": "^4.1.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
